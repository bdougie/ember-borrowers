<h1>Friends Index</h1>
<h2>Friends: {{length}}</h2>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each}}
      <tr>
        <td>{{link-to fillName "friends.show" this}}</td>
        <td><a href="#" {{ction "delete" this}}>Delete</a></td>
      </tr>
      {{/each}}
  </tbody>
</table>

